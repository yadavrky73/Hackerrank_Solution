def countSort(arr):
   
    a = [[]for i in range(100)]
    for i in range(n//2):
       
        
        a[int(arr[i][0])].append('-')
            
    for i in range(n//2,n):    
        
        a[int(arr[i][0])].append(arr[i][1])
    for i in a:
        if i:
            print(*i,end=' ')
            

if __name__ == '__main__':
    n = int(input().strip())

    arr = []

    for _ in range(n):
        arr.append(input().rstrip().split())

    countSort(arr)
